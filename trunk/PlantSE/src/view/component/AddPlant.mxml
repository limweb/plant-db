			protected function ListID_changeHandler(event:ListEvent):void
			{
				vo = ListID.selectedItem as PlantDBVO;
				action = "EDIT";
				setData();
			}
			
			private function setData():void {
					txtCommonName.text = vo.commonname;
					txtOtherName.text = vo.othername;
					txtSciName.text = vo.scientificname;
					txtGenus.text = vo.genus;
					txtFamily.text = vo.family;
					txtTexture.text = vo.texture;
					txtLeaf.text = vo.leaf;
					stepperMinHeight.value = vo.minheight;
					stepperMaxHeight.value = vo.maxheight;
					stepperMinCrownSize.value = vo.minhedgesize;
					stepperMaxCrownSize.value = vo.maxhedgesize;
					stepperShortPlantDist.value = vo.closeplantingdist;
					stepperLongPlantDist.value = vo.longplantingdist;
					radiogroupUnit.selectedValue = vo.unit;
					radiogroupIsFruit.selectedValue = vo.isfruit;
					radiogroupIsHerb.selectedValue = vo.isherb;
					radiogroupIsScented.selectedValue = vo.isscented;
					radiogroupEvergreen.selectedValue = vo.deciduousorevergreen;
					radiogroupPrivacyScreen.selectedValue = vo.isscreen;
					radiogroupMakeNoise.selectedValue = vo.makenoise;
					radiogroupSafeForKids.selectedValue = vo.safeforkids;
					
					var i:int = 0;
					
					
					for(i=0;i<FlowerColorList.dataProvider.length;i++){
						if(FlowerColorList.dataProvider[i].data == vo.flowercolor){
							FlowerColorList.selectedIndex = i;
							break;
						}
					}
					
					var encodedBytes:String;
					var baseDecoder:Base64Decoder = new Base64Decoder();

					if(vo.pic1 != null){
					b64img1 = vo.pic1;
					encodedBytes = vo.pic1;
					baseDecoder.decode(encodedBytes);
					img1.source = baseDecoder.toByteArray();
					} else {
						img1.source = null;
						b64img1  = null;
					}
					
					
					b64img2 = vo.pic2;
					encodedBytes = vo.pic2;
					baseDecoder.decode(encodedBytes);
					img2.source = baseDecoder.toByteArray();
					
					
					
					b64img3 = vo.pic3;
					encodedBytes = vo.pic3;
					baseDecoder.decode(encodedBytes);
					img3.source = baseDecoder.toByteArray();
					
					
					
					//comboTreeSize.selectedItem = vo.treesize;
					
			}
			
			private function getData():PlantDBVO {
				vo.commonname = txtCommonName.text;
				vo.othername = txtOtherName.text;
				vo.scientificname = txtSciName.text;
				vo.genus = txtGenus.text;
				vo.family = txtFamily.text;
				vo.texture = txtTexture.text;
				vo.leaf = txtLeaf.text;
				vo.pic1 = b64img1;
				vo.pic2 = b64img2;
				vo.pic3 = b64img3;
				
				//vo.treesize = comboTreeSize.selectedItem;
				return vo;
			}
			
			protected function SaveClick(event:MouseEvent):void
			{
				
				if(action == "NEW"){
					if(StringUtil.trim(getNewData().commonname).length > 0 ){
						FlexGlobals.topLevelApplication.dao.setConnection(FlexGlobals.topLevelApplication.myDb.connection);
						FlexGlobals.topLevelApplication.dao.insertRow(getNewData(),resultfunc);
					} else {
						Alert.show("ใส่ชื่อด้วย");
					}
				} else if(action == "EDIT" ){
					if(StringUtil.trim(getData().commonname).length > 0){
					FlexGlobals.topLevelApplication.dao.setConnection(FlexGlobals.topLevelApplication.myDb.connection);
					FlexGlobals.topLevelApplication.dao.updateRow(getData(),resultUpdate);
					}else {
						Alert.show("ใส่ชื่อด้วย");
					}
					
				}else {
					
				}
				action = null;
				ClearAll();				
				
			}
			
			private function getNewData():PlantDBVO {
				var newvo:PlantDBVO  = new PlantDBVO();
				newvo.commonname = txtCommonName.text;
				newvo.flowercolor = FlowerColorList.selectedItem.data;
				newvo.pic1 = b64img1;
				newvo.pic2 = b64img2;
				newvo.pic3 = b64img3;
				return newvo;
			}
			
