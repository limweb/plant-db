			protected function spinnerClickHandler(event:MouseEvent):void
			{
				// TODO Auto-generated method stub
				var cb:CheckBox = event.target as CheckBox;
				
				if (cb.id == "maxHeightEnable"){
					maxHeightStepper.enabled = cb.selected;
				}else if (cb.id == "minHeightEnable"){
					minHeightStepper.enabled = cb.selected;
				}else if (cb.id == "minCrownEnable"){
					minCrownSizeStepper.enabled = cb.selected;
				}else if (cb.id == "maxCrownEnable"){
					maxCrownSizeStepper.enabled = cb.selected;
				}else if (cb.id == "closeEnable"){
					closeRangeStepper.enabled = cb.selected;
					unitCombo1.enabled = cb.selected;
				}else if (cb.id == "longEnable"){
					longRangeStepper.enabled = cb.selected;
